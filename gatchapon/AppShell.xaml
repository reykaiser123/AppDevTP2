<?xml version="1.0" encoding="UTF-8" ?>

<Shell
    x:Class="gatchapon.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:gatchapon"
    Title="gatchapon"
    Shell.TitleColor="#F0EAD2"
    Shell.ForegroundColor="#F0EAD2"
    Shell.BackgroundColor="#ADC179"
    
	>
    <ShellContent
        Title=""
        Route="Login"
        ContentTemplate="{DataTemplate local:Login}"
        Shell.NavBarIsVisible="False"
        Shell.FlyoutBehavior="Disabled"/>
    <ShellContent
        Route="Dashboard"
        ContentTemplate="{DataTemplate local:Dashboard}"
        Shell.NavBarIsVisible="False"
        Shell.FlyoutBehavior="Disabled"/>





</Shell>
    