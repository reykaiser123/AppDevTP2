<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="gatchapon.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:gatchapon"
    Title="gatchapon">

  
    <ShellContent
        Title="Login"
        Route="Login"
        Shell.NavBarIsVisible="False">
        <ShellContent.ContentTemplate>
            <DataTemplate>
                <local:Login />
            </DataTemplate>
        </ShellContent.ContentTemplate>
    </ShellContent>

    <ShellContent
    Title="dashboard"
    Route="dashboard"
    Shell.NavBarIsVisible="False">
        <ShellContent.ContentTemplate>
            <DataTemplate>
                <local:Dashboard />
            </DataTemplate>
        </ShellContent.ContentTemplate>
    </ShellContent>


</Shell>
