    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:gatchapon"
    Title="gatchapon">

    <ShellContent
        Title="Login"
        Route="Login">
        <ShellContent.ContentTemplate>
            <DataTemplate>
                <local:Login />
            </DataTemplate>
        </ShellContent.ContentTemplate>
    </ShellContent>

</Shell>
